        //     "type": "uptime"
        // },
        {
            "key": "{#39}│󱂬 desktop {#keys} {#39}│",
	    "type": "wm"
        },
        {
            "key":  "{#39}│ term    {#keys} {#39}│",
            "type": "terminal"
        },
        {
            // "key": "│ {#32} shell   {#keys}│",
            "key": "{#39}│ shell   {#keys} {#39}│",
            "type": "shell"
        },
	{
            "key": "{#39}│󰛖 font    {#keys} {#39}│",
            "type": "terminalfont",
        },
        // {
        //     "key": "│ {#31}󰍛 cpu     {#keys}│",
        //     "type": "cpu",
        //     "showPeCoreCount": true
        // },
        // {
        //     "key": "│ {#31}󰍛 gpu     {#keys}│",
        //     "type": "gpu",
        // },
        {
            "key": "{#39}│󰍹 display {#keys} {#39}│",
            "type": "display",
        },

// ~/.config/fastfetch/config.jsonc
{
    "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
    "logo":
    {
         "type": "kitty",
         "source": "~/.config/fastfetch/vagabondi.jpg",
         "width": 28,
         "height": 15,
         "padding": {
            "top": 1
          }
    },
    "display": {
        "separator": " "
    },
    "modules": [
        {
            "key": "{#39}            ",
            "type": "custom"
        },
        {
            "key": "{#39}╭───────────╮",
            "type": "custom"
        },
        {
            "key": "{#39}│{icon} distro  {#keys} {#39}│",
            "type": "os"
        },
        {
            "key": "{#39}│ kernel  {#keys} {#39}│",
            "type": "kernel"
        },
        {
            "key": "{#39}│󰏔 package {#keys} {#39}│",
            "type": "packages",
        },
        {
            "key": "{#39}│ user    {#keys} {#39}│",
            "type": "title",
            "format": "{user-name}@{host-name}"
        },
        // {
        //     "key": "│ {#32}󰇅 hname   {#keys}│",
        //     "type": "title",
        //     "format": "{host-name}"
        // },
        // {
